
import java.io.*;

class Demo{
      public static void main(String a[]){
        System.out.println("Enter time in sec");
        int sec = Integer.parseInt(a[0]);
        int rem = sec/3600;
        int rem1 = sec%3600;
        int rem2 = rem1/60;
        int rem3 = rem1%60;
        System.out.println(rem + ":" + rem2 + ":" + rem3);
   }
}
