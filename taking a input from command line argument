import java.io.*;

class Demo{
      public static void main(String a[]){
             String str = a[0];
             System.out.println(str + " " + "Welcome in java programming");
   }
}



# Integer input from command line argument

import java.io.*;

class Demo{
      public static void main(String a[]){
             int i = Integer.parseInt(a[0]);
             System.out.println("Given Number is" + " " + i);
   }
}




